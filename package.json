{
  "name": "bolivar-angular20-migration",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "config:env": "ts-node ./src/app/core/setenv.config.ts",
    "config:env:dev": "npm run config:env -- --environment=dev",
    "config:env:stage": "npm run config:env -- --environment=stage",
    "config:env:prod": "npm run config:env -- --environment=prod",
    "serve": "ng serve --poll=2000",
    "serve:clean": "ng serve --poll=2000 --delete-output-path",
    "start": "npm run config:env:dev && npm run serve",
    "build": "ng build --configuration",
    "build:dev": "npm run config:env:dev && npm run build development",
    "build:stage": "npm run config:env:stage && npm run build stage",
    "build:prod": "npm run config:env:prod && npm run build",
    "deploy:firebase": "npm run build:prod && firebase deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "firebase-tools": "^14.17.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.0",
    "yargs": "^17.7.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{.ts,html,scss,md,json}": "prettier --write"
  }
}
